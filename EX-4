import java.util.Vector;
class VectorExample {
 public static void main(String[] args) {
 Vector<String> fruits = new Vector<>();
 fruits.add("Apple");
 fruits.add("Banana");
 fruits.add("Mango");
 fruits.add("Orange");
 System.out.println("Fruits in the vector:");
 for (String ff : fruits) {
 System.out.println(ff);
 }
 fruits.remove("Banana");
 System.out.println("\nAfter removing 'Banana':");
 for (String ff : fruits) {
 System.out.println(ff);
 }
 System.out.println("\nFruit at index 1: " + fruits.get(1));
 System.out.println("Total number of fruits: " + fruits.size());
 }
}
