import java.applet.Applet;
import java.awt.*;
/*
<html>
  <body>
    <applet code="HumanFaceApplet.class" width="350" height="400">
      Your browser does not support Java Applets.
    </applet>
  </body>
</html>
*/
public class HumanFaceApplet extends Applet {
public void paint(Graphics g) {
        setBackground(Color.lightGray);
     g.setColor(Color.white);
        g.drawOval(50, 50, 200, 220);
        g.setColor(Color.white);
        g.fillOval(90, 110, 50, 30);  
        g.fillOval(180, 110, 50, 30); 
        g.setColor(Color.black);
        g.fillOval(110, 115, 15, 15);
        g.fillOval(200, 115, 15, 15); 
        g.drawLine(150, 150, 140, 190);
        g.drawLine(150, 150, 160, 190);
        g.drawLine(140, 190, 160, 190);
        g.setColor(Color.red);
        g.drawArc(110, 200, 90, 40, 180, 180); // Smile
            
    }
}
